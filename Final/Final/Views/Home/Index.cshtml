                                            <ul>
                                                <li><i class="fa fa-clock-o"></i>@item.CreatedDate.ToString("dd.MM.yyyy")</li>
                                            </ul>
                                        </div>
                                        <h6 class="title"><a asp-controller="Category" asp-action="Details">@item.Title</a></h6>
                                    </div>
                                </div>
                            </div>
                        </div>

