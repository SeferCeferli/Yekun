
@model VmNews
@{

    ViewData["Title"] = "Home Page";
}

<main>
    <!-- News Post Start -->
    <section id="news">
        <div class="container">
            <div class="row">
                <div class="col-lg-6">
                    @foreach (var item in Model.news.OrderByDescending(v=>v.CreatedDate).Take(1))
                    {
                        <div class="single-post-wrap style-overlay">
                            <div class="thumb">
                                <img src="~/Uploads/Images/@item.MainImage" style="width:100%" alt="">
                                <a class="tag-base tag-blue" href="#">@item.Category.Name</a>
                            </div>
                            <div class="details">
                                <div class="post-meta-single">
                                    <p><i class="fa fa-clock-o"></i>@item.CreatedDate.ToString("dd.MM.yyyy")</p>
                                </div>
                                <h5 class="title"><a asp-controller="Category" asp-action="Details">@item.Title</a></h5>
                            </div>
                        </div>
                    }

                <div class="row midlle">
                    @foreach (var item in Model.news.OrderByDescending(a=>a.CreatedDate).Skip(1).Take(2))
                    {
                        <div class="col-lg-6 col-sm-6">
                            <div class="single-post-wrap style-overlay">
                                <div class="thumb">
                                    <img src="~/Uploads/Images/@item.MainImage" alt="img">
                                </div>
                                <div class="details">
                                    <div class="post-meta-single">
                                        <p><i class="fa fa-clock-o"></i>@item.CreatedDate.ToString("dd.MM.yyyy")</p>
                                    </div>
                                    <h6 class="title"><a asp-controller="Category" asp-action="Details">@item.Title</a></h6>
                                </div>
                            </div>
                        </div>

                    }
                </div>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <div class="Latest-title  mb-4">
                        <h6 class="title">Latest News</h6>
                    </div>
